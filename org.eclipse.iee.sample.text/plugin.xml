<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.eclipse.iee.sample.text.addCommand"
            name="Add Text">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.iee.sample.text.handlers.AddTextHandler"
            commandId="org.eclipse.iee.sample.text.addCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commandImages">
      <image
            commandId="org.eclipse.iee.sample.text.addCommand"
            icon="icons/textForeground.ico">
      </image>
   </extension>



	
</plugin>
