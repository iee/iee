<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.eclipse.iee.sample.image.addCommand"
            name="Add Image">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.iee.sample.image.handlers.AddImageHandler"
            commandId="org.eclipse.iee.sample.image.addCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commandImages">
      <image
            commandId="org.eclipse.iee.sample.image.addCommand"
            icon="icons/cat.gif">
      </image>
   </extension>


   

</plugin>
