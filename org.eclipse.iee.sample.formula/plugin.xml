<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>


 <extension
       point="org.eclipse.ui.commandImages">
    <image
          commandId="org.eclipse.iee.sample.formula.addCommand"
          icon="icons/math.jpg">
    </image>
 </extension>
 <extension
       point="org.eclipse.ui.commandImages">
    <image
          commandId="org.eclipse.iee.sample.formula.addSymbolicCommand"
          icon="icons/symbolic.jpg">
    </image>
 </extension> 
 <extension
       point="org.eclipse.ui.commands">
    <command
          id="org.eclipse.iee.sample.formula.addCommand"
          name="Add Formula">
    </command>
 </extension>
  <extension
       point="org.eclipse.ui.commands">
    <command
          id="org.eclipse.iee.sample.formula.addSymbolicCommand"
          name="Add Symbolic">
    </command>
 </extension>
 <extension
       point="org.eclipse.ui.handlers">
    <handler
          class="org.eclipse.iee.sample.formula.handlers.AddFormulaHandler"
          commandId="org.eclipse.iee.sample.formula.addCommand">
    </handler>
 </extension>
  <extension
       point="org.eclipse.ui.handlers">
    <handler
          class="org.eclipse.iee.sample.formula.handlers.AddSymbolicHandler"
          commandId="org.eclipse.iee.sample.formula.addSymbolicCommand">
    </handler>
 </extension>
  <extension
        point="org.eclipse.ui.bindings">
     <key
           commandId="org.eclipse.iee.sample.formula.addCommand"
           contextId="org.eclipse.ui.contexts.window"
           schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
           sequence="M1+M3+N">
     </key>
     <key
           commandId="org.eclipse.iee.sample.formula.addSymbolicCommand"
           contextId="org.eclipse.ui.contexts.window"
           schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
           sequence="M1+M3+S">
     </key>
  </extension>

	
</plugin>
